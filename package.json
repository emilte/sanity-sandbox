    "react-dom": "^18.2.0",
    "sanity": "^3.57.2",
    "sanity-plugin-documents-pane": "^2.3.0",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.2",
    "@sanity/eslint-config-studio": "^4.0.0",
    "@types/react": "^18.0.25",
    "eslint": "^8.6.0",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
